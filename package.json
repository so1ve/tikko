{
  "name": "tikko",
  "version": "0.0.5",
  "packageManager": "pnpm@7.18.0",
  "author": "Ray <nn_201312@163.com> (@so1ve)",
  "description": "A project helper.",
  "keywords": [],
  "type": "module",
  "homepage": "https://github.com/so1ve/tikko#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/tikko.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/tikko/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "tikko": "bin/tikko.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && puild",
    "watch": "puild --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/ni": "^0.18.8",
    "@clerc/plugin-completions": "^0.8.7",
    "@clerc/plugin-help": "^0.8.7",
    "@clerc/plugin-not-found": "^0.8.7",
    "@clerc/toolkit": "^0.8.7",
    "clerc": "^0.8.7",
    "giget": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.20.5",
    "@so1ve/eslint-config": "^0.32.1",
    "@types/node": "^18.11.11",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "pnpm": "^7.18.0",
    "puild": "^1.3.2",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.5",
    "vitest": "^0.25.4"
  }
}
